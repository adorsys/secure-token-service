{
  "id": "moped",
  "realm": "moped",
  "displayName": "MOPED Realm",
  "displayNameHtml": "<div class=\"kc-logo-text\"><span>MOPED</span></div>",
  "enabled": true,
  "browserFlow": "ad browser",
  "directGrantFlow": "ad direct grant",
  "authenticationFlows": [
    {
      "alias": "ad browser",
      "description": "(adorsys) browser based authentication",
      "providerId": "basic-flow",
      "topLevel": true,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "requirement": "REQUIRED",
          "priority": 0,
          "flowAlias": "ad browser forms",
          "userSetupAllowed": false,
          "autheticatorFlow": true
        }
      ]
    },
    {
      "alias": "ad browser forms",
      "description": "(adorsys) Username, password, otp and other auth forms.",
      "providerId": "basic-flow",
      "topLevel": false,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "custom-auth-username-password-form",
          "requirement": "REQUIRED",
          "priority": 0,
          "userSetupAllowed": false,
          "autheticatorFlow": false
        },
        {
          "authenticator": "auth-otp-form",
          "requirement": "OPTIONAL",
          "priority": 1,
          "userSetupAllowed": false,
          "autheticatorFlow": false
        }
      ]
    },
    {
      "alias": "ad direct grant",
      "description": "(adorsys) OpenID Connect Resource Owner Grant",
      "providerId": "basic-flow",
      "topLevel": true,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "custom-direct-access-authenticator",
          "requirement": "REQUIRED",
          "priority": 20,
          "userSetupAllowed": false,
          "autheticatorFlow": false
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "moped-client",
      "name": "moped-client",
      "description": "Moped-Client",
      "surrogateAuthRequired": false,
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "my-special-client-secret",
      "redirectUris": [
        "*"
      ],
      "webOrigins": [
        "*"
      ],
      "notBefore": 0,
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "publicClient": true,
      "frontchannelLogout": false,
      "protocol": "openid-connect",
      "attributes": {},
      "fullScopeAllowed": false,
      "nodeReRegistrationTimeout": -1,
      "useTemplateConfig": false,
      "useTemplateScope": false,
      "useTemplateMappers": false,
      "protocolMappers": [
        {
          "name": "Custom Claim Mapper",
          "protocol": "openid-connect",
          "protocolMapper": "custom-claim-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "firstName",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "given_name",
            "jsonType.label": "String"
          }
        }
      ]
    }
  ],
  "components": {
    "org.keycloak.keys.KeyProvider": [
      {
        "name": "rsa-generated",
        "providerId": "rsa-generated",
        "config": {
          "keySize": [
            "4096"
          ]
        }
      },
      {
        "name": "hmac-generated",
        "providerId": "hmac-generated",
        "config": {
          "secretSize": [
            "512"
          ]
        }
      }
    ],
    "org.keycloak.storage.UserStorageProvider": [
      {
        "name": "custom-user-storage-provider",
        "providerId": "custom-user-storage-provider",
        "subComponents": {},
        "config": {
          "vendor": [
            "adorsys"
          ],
          "priority": [
            "0"
          ],
          "importEnabled": [
            "false"
          ],
          "editMode": [
            "READ_ONLY"
          ],
          "syncRegistrations": [
            "false"
          ],
          "connectionUrl": [
            "${stsLoginUrl}"
          ],
          "stsLoginUrl": [
            "http://sts:8888/login"
          ],
          "evictionHour": [
            "0"
          ],
          "evictionMinute": [
            "0"
          ],
          "cachePolicy": [
            "NO_CACHE"
          ],
          "enabled": [
            "true"
          ]
        }
      }
    ]
  }
}
